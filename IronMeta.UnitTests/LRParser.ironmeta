ironmeta LRParser<char, string> : IronMeta.Matcher.CharMatcher<string>
{

	A = A:a A:b -> { "(" + (string)a + (string)b + ")" } | 'a' -> { "a" };

	Exp = Exp:e "+" Term:t -> { "(" + (string)e + " + " + (string)t + ")" } | Term;
	Term = "1" -> { "1" };

	NonLR = NonLR_A NonLR_B;

	NonLR_A = "a";
	NonLR_B = "b" | NonLR_C;
	NonLR_C = "c";

	AAA = AAA*:x "a":y -> { "(" + (string)x + "a)" } | "b":z -> { "b" };

}
